{
  "name": "lion",
  "version": "2.0.0",
  "description": "Discord bot utilized by UCF Computer Science Discord",
  "main": "index.ts",
  "scripts": {
    "test": "jest --verbose",
    "test:auto": "nodemon -e ts -w ./src/test -x npm run test:serve",
    "test:serve": "node -r ts-node/register -r dotenv/config --inspect src/test/tester.ts",
    "build": "tsc --project tsconfig.prod.json",
    "watch": "nodemon -e ts -w ./src -x npm run watch:serve",
    "watch:serve": "node -r ts-node/register --inspect src/index.ts",
    "lint": "eslint . --ext .ts",
    "generate": "node ./scripts/generate.js --"
  },
  "author": "joey",
  "license": "MIT",
  "dependencies": {
    "@types/cheerio": "^0.22.29",
    "@types/express": "^4.17.12",
    "@types/js-levenshtein": "^1.1.0",
    "@types/ws": "^8.5.5",
    "axios": "^1.4.0",
    "bottlejs": "^2.0.1",
    "cheerio": "^1.0.0-rc.3",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jest-discordjs-mocks": "^1.0.2",
    "js-levenshtein": "^1.1.6",
    "mongoose": "^7.3.1",
    "ms": "^2.1.2",
    "winston": "^3.3.3",
    "winston-papertrail": "^1.0.5",
    "words-to-numbers": "^1.5.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/ms": "^0.7.31",
    "@types/node": "^20.3.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^29.6.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.3.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.0.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
